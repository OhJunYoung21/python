def sort(new_list):
    i = 0
    num_1 = len(new_list)
    for i in range(num_1):
        for j in range(num_1 - 1):
            if new_list[j] < new_list[j + 1]:
                j += 1
            elif new_list[j] >= new_list[j + 1]:
                temp = new_list[j]
                new_list[j] = new_list[j + 1]
                new_list[j + 1] = temp
                j += 1
    return new_list

print(sort([2,4,1,3,7]))
