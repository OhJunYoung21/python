import random
import time 
from time import sleep

lunch = ['hamburger', 'pizza', 'coke', 'sprite']
while True:
    print(lunch)
    item = input("enter menu:")
    if item == 'q':
        break
    else:
        lunch.append(item)

set_lunch = set(lunch)
while True:
    item_1 = input("delete menu:")
    if item_1 == 'q':
        break
    else:
        set_lunch = set_lunch - set([item_1])
        print(set_lunch)
print(lunch, "choose among these menu")
print(5)
time.sleep(1)
print(4)
time.sleep(1)
print(3)
time.sleep(1)
print(2)
time.sleep(1)
print(1)
time.sleep(1)
menu = random.choice(list(lunch))
print(menu)
