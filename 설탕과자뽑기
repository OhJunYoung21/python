w, h = list(map(int, input().split()))
n = int(input())

new_list = []
for i in range(w):
    new_list.append([])
    for j in range(h):
        new_list[i].append(0)

for i in range(n):
    l, d, x, y = list(map(int, input().split()))
    for j in range(l):
        if d == 0:
            new_list[x-1][y - 1 + j] = 1
        elif d == 1:
            new_list[x - 1 + j][y-1] = 1

for i in range(w):
    for j in range(h):
        print(new_list[i][j], end=' ')
    print()
